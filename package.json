{
  "name": "St-Gospel",
  "description": "Reading Gospel groups",
  "version": "1.0.0",
  "main": "lib/main.js",
  "scripts": {
    "index": "webpack serve --hot --config webpack.config.dev.js",
    "build": "webpack --config webpack.config.js",
    "static": "python manage.py collectstatic --noinput",
    "scss": "webpack --config webpack.config.sass.js",
    "test": "jest",
    "i18n-ru": "webpack --config webpack.config.i18n.ru.js",
    "compile": "babel jsx -d lib",
    "br-build": "browserify lib/main.js -o gospel/static/js/gospel.bundle.js",
    "br-prod": "browserify lib/main.js -g [ envify --NODE_ENV production ] -g uglifyify -p bundle-collapser/plugin | uglifyjs --compress --mangle > gospel/static/js/gospel.bundle.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^8.3.0",
    "babel-preset-minify": "^0.5.2",
    "browserify": "^17.0.0",
    "bundle-collapser": "^1.4.0",
    "circular-dependency-plugin": "^5.2.2",
    "css-loader": "^5.2.7",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "envify": "^4.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.33.2",
    "file-loader": "^6.2.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.6.2",
    "optimize-css-assets-webpack-plugin": "^5.0.8",
    "sass": "^1.69.5",
    "sass-loader": "^11.1.1",
    "uglify-js": "^3.17.4",
    "uglifyify": "^5.0.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^3.11.3",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "@tinymce/tinymce-react": "^3.14.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
